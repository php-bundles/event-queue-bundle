{
    "name": "symfony-bundles/event-queue-bundle",
    "type": "symfony-bundle",
    "description": "Symfony EventQueue Bundle",
    "keywords": ["event", "queue", "dispatcher", "redis", "symfony", "bundle"],
    "homepage": "https://github.com/symfony-bundles/event-queue-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Khaperets",
            "email": "khaperets@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SymfonyBundles\\EventQueueBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=5.6",
        "symfony/console": "^3.0",
        "symfony-bundles/queue-bundle": "^1.0",
        "symfony-bundles/bundle-dependency": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "phpunit/php-code-coverage": "^3.3.0|^4.0"
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
